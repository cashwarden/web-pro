{
  "name": "cashwarden-web",
  "version": "0.13.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng s -o",
    "hmr": "ng s -o --hmr",
    "build": "npm run ng-high-memory build",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@antv/g2plot": "^1.1.28",
    "@delon/abc": "^12.0.2",
    "@delon/acl": "^12.0.2",
    "@delon/auth": "^12.0.2",
    "@delon/cache": "^12.0.2",
    "@delon/chart": "^12.0.2",
    "@delon/form": "^12.0.2",
    "@delon/mock": "^12.0.2",
    "@delon/theme": "^12.0.2",
    "@delon/util": "^12.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ajv": "^8.6.1",
    "ajv-formats": "^2.1.0",
    "file-saver": "^2.0.5",
    "ng-alain": "12.0.2",
    "ng-zorro-antd": "~12.0.1",
    "ngx-tinymce": "^12.0.0",
    "ngx-ueditor": "^12.0.0",
    "rxjs": "~6.5.5",
    "screenfull": "^5.0.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.2",
    "@angular-eslint/builder": "~12.2.2",
    "@angular-eslint/eslint-plugin": "~12.2.2",
    "@angular-eslint/eslint-plugin-template": "~12.2.2",
    "@angular-eslint/schematics": "~12.2.2",
    "@angular-eslint/template-parser": "~12.2.2",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/language-service": "~12.1.1",
    "@delon/testing": "^12.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.7",
    "@types/node": "^12.19.8",
    "@typescript-eslint/eslint-plugin": "~4.23.0",
    "@typescript-eslint/parser": "~4.23.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^2.2.1",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsdoc": "~36.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-prettier": "^2.2.1",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.1.2",
    "ng-alain": "^12.0.2",
    "ng-alain-plugin-theme": "^12.0.0",
    "ng-alain-sts": "^0.0.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.3.5",
    "webpack-bundle-analyzer": "^3.9.0",
    "xlsx": "^0.16.9"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  }
}
